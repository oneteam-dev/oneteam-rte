{
  "name": "oneteam-rte",
  "version": "2.0.3",
  "description": "Oneteam Rich Text Editor.",
  "repository": {
    "type": "git",
    "url": "git@github.com:oneteam-dev/oneteam-rte.git"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "lib",
    "es"
  ],
  "scripts": {
    "start": "webpack-dev-server --progress",
    "lint": "eslint src",
    "test": "npm run lint",
    "watch": "babel -w -d lib src",
    "clean:lib": "if [ -d lib ]; then rm -rf lib; fi",
    "clean:es": "if [ -d es ]; then rm -rf es; fi",
    "prebuild": "npm run clean:lib && npm run clean:es",
    "build": "npm run build:commonjs && npm run build:es",
    "build:es": "BABEL_ENV=es babel -d es src",
    "build:commonjs": "BABEL_ENV=commonjs babel -d lib src",
    "prepublishOnly": "npm run lint && npm run build",
    "publish-if-tagged": "/bin/bash scripts/npm-publish.sh",
    "bump-patch-number": "(git branch | grep \\*\\ master > /dev/null) && npm version patch -m 'Version bump to %s' && git push --tags && git push origin master"
  },
  "author": "Oneteam Inc.",
  "contributors": [
    {
      "name": "Atsushi NAGASE",
      "email": "a@ngs.io",
      "url": "http://ngs.io/"
    },
    {
      "name": "Shingo Sato",
      "email": "shinsugar@gmail.com",
      "url": "https://sugarshin.net/"
    }
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.0.0 || ^15.0.0",
    "react-dom": "^16.0.0 || ^15.0.0"
  },
  "dependencies": {
    "@sugarshin/draft-js-emoji-plugin": "^2.0.1-oneteam.0",
    "classnames": "^2.2.5",
    "draft-js": "^0.10.5",
    "draft-js-block-breakout-plugin": "^2.0.1",
    "draft-js-checkable-list-plugin": "^4.0.0",
    "draft-js-linkify-plugin": "^2.0.1",
    "draft-js-markdown-shortcuts-plugin": "^0.4.0",
    "draft-js-mention-plugin": "^2.0.1",
    "draft-js-oneteam-rte-plugin": "^0.4.1",
    "draft-js-plugins-editor": "^2.0.4",
    "draft-js-prism": "^1.0.5",
    "emojione": "^2.2.7",
    "immutable": "~3.7.4",
    "lodash": "^4.17.4",
    "prismjs": "^1.10.0",
    "prop-types": "^15.6.0",
    "react-bootstrap": "^0.32.1",
    "react-oneteam": "^0.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.39",
    "@babel/core": "^7.0.0-beta.39",
    "@babel/polyfill": "^7.0.0-beta.39",
    "@babel/preset-es2015": "^7.0.0-beta.39",
    "@babel/preset-es2016": "^7.0.0-beta.39",
    "@babel/preset-es2017": "^7.0.0-beta.39",
    "@babel/preset-react": "^7.0.0-beta.39",
    "@babel/preset-stage-2": "^7.0.0-beta.39",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.0-beta.0",
    "css-loader": "^0.28.9",
    "eslint": "^4.16.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.6.1",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "prism-github": "^1.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-fa": "^5.0.0",
    "react-hot-loader": "^4.0.0-beta.18",
    "style-loader": "^0.20.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  }
}
